{
    "name": "tcytra/enraiged-laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-exif": "*",
        "ext-gd": "*",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.5.4",
        "laravel-enso/enums": "^2.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "league/glide-laravel": "^1.0",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Enraiged\\": "packages/enraiged/src/",
            "Enraiged\\Accounts\\": "packages/enraiged-accounts/src/",
            "Enraiged\\Agreements\\": "packages/enraiged-agreements/src/",
            "Enraiged\\Avatars\\": "packages/enraiged-avatars/src/",
            "Enraiged\\Exports\\": "packages/enraiged-exports/src/",
            "Enraiged\\Files\\": "packages/enraiged-files/src/",
            "Enraiged\\Forms\\": "packages/enraiged-forms/src/",
            "Enraiged\\Profiles\\": "packages/enraiged-profiles/src/",
            "Enraiged\\Tables\\": "packages/enraiged-tables/src/"
        },
        "files": [
            "packages/enraiged/helpers/auth.php",
            "packages/enraiged/helpers/dates.php",
            "packages/enraiged/helpers/utility.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "compile": [
            "@php artisan migrate:fresh --seed"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
